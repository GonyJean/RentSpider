{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "prettier"
  ],
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "array-callback-return":0,
    "no-const-assign":0,
    "no-useless-escape":0,
    "prefer-template":0,
    "no-empty":0,
    "prefer-destructuring":0,
    "no-undef":0,
    "react/no-unused-state":0,
    "one-var":0,
    "default-case":0,
    "no-fallthrough":0,
    "no-plusplus":0,
    "react/no-did-mount-set-state":0,
    "react/sort-comp":0,
    "no-return-assign":0,
    "react/no-array-index-key":0,
    "no-alert":0,
    "no-unused-expressions":0,
    "no-param-reassign":0,
    "no-unused-vars":0,
    "class-methods-use-this":0,
    "no-underscore-dangle":0,
    "react/prefer-stateless-function":0,
    "no-useless-constructor":0,
    "import/first":0,
    "import/no-unresolved":0,
    "import/extensions":0,
    "consistent-return":0,
    "eqeqeq":0,
    "react/jsx-no-bind":0,
    "arrow-parens": 0,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "no-console": 0,
    "no-mixed-operators": 0,
    "no-use-before-define": 0,
    "radix": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "semi": [2, "never"]
  }
}
